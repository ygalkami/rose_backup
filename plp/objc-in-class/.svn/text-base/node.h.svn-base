#import <objc/Object.h>

@interface Node : Object {
    char* value;
    Node* leftChild;
    Node* rightChild;
}

-(Node*) initWith: (char*) val;
-(char*) print;
-(void) setVal: (char*) val;
-(void) setLeft: (Node*) left;
-(void) setRight: (Node*) right;
-(void) preOrder;
-(void) inOrder;
-(void) postOrder;

@end
